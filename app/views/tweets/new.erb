<% if logged_in? %>
<%= @error %>
  <form action='/tweets' method="POST">
    Content:
    <input type="text" name="content"><br><br>
    Create Your Tweet:

    <input type="submit" name="submit" id="submit" value="Post Tweet">

  </form>

  <% else %>
      <h1> You Must Be Logged in to Make Tweets </h1>
  <% end %>
